using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using Vida.ServiceFacade.ServiceFacades.Procedimento.Misc;

namespace Vida.DAO.Procedimento.Misc
{
    public class GrupoCBODAO : VidaServiceFacadeDAO, IGrupoCBO
    {
        T IGrupoCBO.BuscarPorCBO<T>(string co_cbo)
        {
            string hql = "select cbo.GrupoCBO from Vida.Model.CBO cbo where cbo.Codigo = '"+ co_cbo+"'";
            return Session.CreateQuery(hql).UniqueResult<T>();
        }
    }
}
