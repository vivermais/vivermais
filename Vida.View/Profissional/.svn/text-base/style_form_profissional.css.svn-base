/* Estilos do formulário - MODULO PROFISSIONAL */

body {
	background-color: #ffffff;
}

.botoesroll 
{
    float:left;
    width:auto;
    padding:0px 20px 0 0;
    text-decoration: none;
    margin-top:10px;
}



#divbotoesposition
{
    padding:15px 0 0 0;
}

#bgprofissional
{
    background-color: #ffffff;
    width:100%;
    height:65px;
    
}



#top	{
    width: 813px;
	height: auto;
	float: inherit;
	margin: 0 auto;
	z-index: auto;
	padding-right:60px;

}

h2{

    font-family:arial;
    margin: 0px 0px 0px 0px;
    padding:22px 0px 0px 50px;
    Background: url( 'img/h2.png' ) no-repeat left;
    color: #333333;
    height:48px;
    width:810px;
    font-size:24px;
    font-weight:normal;

}

h4{
    font-family: Arial, Helvetica, sans-serif;
    font-size:11px;
}


br{
clear: left;
}

legend  {
	font-family:Arial, Helvetica, sans-serif;
	font-size: 13px;
	letter-spacing: 0px;
	font-weight: bold;
	line-height: 1.2;
	color: #ffffff;
	padding: 3px 12px;
	background: #422852;
	padding-bottom:10px;
	margin-top:30px;
	margin-bottom:30px;
}

fieldset {
	border:1px dashed #422852;
	padding:10px;
	margin-bottom:20px;
}

.formulario
{
  width:700px;
  height:auto;
  /*margin-left: 117px;*/
  margin-right:0;
  padding: 10px 10px 20px 10px;
}

.rotulo
{
	float:left;
	height: 17px;
	font-size: 10px;
	color: #ffffff;
	background-color:#6b3992;
	border: 1px solid #6b3992;
	padding-bottom:2px;
	padding-left:8px;
	width: 156px;
	line-height: 25px;
	margin-top: 0px;
	margin-left: 1px;
	margin-right:5px;
	font-weight: bold;
		
}

.rotulomin
{
    line-height: 17px;
	/*height: 17px;*/
    font-size: 10px;
    background-color:#6b3992;
    margin-left: 5px;
    border: 1px solid #6b3992;
    /* padding-bottom:2px; */
    padding-right:25px;
    padding-top:6px;
    padding-left:8px;
    margin-bottom:6px;
    color: #ffffff;
}

.colunaEscondida
{
	display:none;
}

.campo
{
	font-family: Arial;
	font-size: 11px;
	border: 1px solid #6b3992;
	line-height: 17px;
	height: 16px;
	margin-top: 0px;
	/*display: list-item;*/
	background:#fbf6fe;
	color:#422852;
	display:inherit;
	margin-right:5px;
	padding-left: 5px;
}

.drop
{
	font-family: Arial;
	font-size: 11px;
	border: 1px solid #6b3992;
	line-height: 17px;
	height: 21px;
	margin-top: 0px; /*display: list-item;*/
	background: #fbf6fe;
	color: #422852;
	display: inherit;
	margin-right: 5px;
	padding-left: 5px;
}

.camporadio
{
	font-family: Arial;
	font-size: 11px;
	height: 23px;
	margin-top: 0px;
	background:#f6f6f6;
	padding-right:50px;
	margin-bottom:0px;
	/*Alinhamento do texto dentro da linha*/
	vertical-align:middle;

}

.camporadio_end
{
	font-family: Arial;
	font-size: 11px;
	height: 23px;
	margin-top: 0px;
	background:#f6f6f6;
	padding-right:40px;
	margin-bottom:0px;
	/*Alinhamento do texto dentro da linha*/
	vertical-align:middle;
}

.radio
{
    font-family: Arial;
	font-size: 10px;
	border: 1px solid #AAA;
}

/*Classe HEADER das tabelas*/
.tab
{
	font-family: Verdana;
	font-size: x-small;
	border: 1px solid #CCC;
	height: 18px;
	background-color:#e7e7e7;
    font-weight:bold;	 
}

/*Classe ROW das tabelas*/
.tabrow
{
	font-family: arial;
	font-size: 8pt;
	border: 1px solid #CCC;
	height: 18px;
	text-align:center;
}

.tabrow_left
{
	font-family: arial;
	font-size: 8pt;
	border: 1px solid #CCC;
	height: 18px;
	
}

.form {
	margin:0;
	padding:0;
}

.buttons {
	background: #e3e3db;
	font-size:12px; 
	color: #333333; 
	padding: 6px 10px;
	border-width: 2px;
	border-style: solid;
	border-color: #fff #d8d8d0 #d8d8d0 #fff;
	text-decoration: none;
	text-transform:uppercase;
	font-weight:bold;
}

div, select { font-family:Arial, Verdana, Helvetica, sans-serif; text-decoration:none; }

.sis_fonte_small { font-size:11px; font-family:Verdana, Arial, Helvetica, sans-serif; }

/* Estilo do Accordeon*/
.accordionHeader
{
    border: 1px solid #142126;
    color: #142126;
    background-color: #eee;
    /*font-weight: bold;*/
    padding: 5px;
    margin-top: 5px;
    cursor: pointer;
    width: 500px;
    
}

.accordionHeaderSelected
{
    border: 1px solid #142126;
    color: white;
    background-color: #142126;
    /*font-weight: bold;*/
    padding: 5px;
    margin-top: 5px;
    cursor: pointer;
    width: 500px;
}

.accordionContent
{
    background-color: #fff;
    border: 1px solid #142126;
    border-top: none;
    padding: 5px;
    padding-top: 10px;
    width: 500px;}
