/* Estilos do Gridview ---------------- */

TABLE.gridview
{
    background-color: #6699FF;
    border: solid 1px #000;
    font-family: Arial;
    font-size: 14px;
    margin-top: 25px;
    width: 100%;
}
TABLE.gridview TR
{
    background-color: #c8dced;
    color: #4b7fac;
    border: solid 1px #fff;
    font-size: 12px;
}

TABLE.gridview TR a
{
    color: #c8dcee;
    font-weight: bold;
}

TABLE.gridview TR a:hover
{
    color: #08807b;
    text-decoration: underline;
    font-weight: bold;
}




TABLE.gridview TR:hover
{
    background-color: #c0d6eb;
    color: #08807b;
}
TABLE.gridview TH
{
    background: #78a4c8;
    color: #FFFFFF;
    padding: 7px 10px 7px 10px;
    font-weight: bold;
    background-image: url(img/bg-head-grid.png);
    font-size: 12px;
}
TABLE.gridview TD
{
    padding: 5px 10px 5px 10px;
    height: 18px;
    border: solid 1px #e5ebf1;
}


.GridviewSelected
{
    background-color: Aqua;
    color: #fff;
}


.GridviewPager
{
    background-color: #999 !important;
    font-weight: bold;
    color: #000;
}


.align-icon
{
    text-align: center;
}

.align-icon:hover
{
    text-align: center;
}

.icon-effect
{
    opacity: 0.85;
    -moz-opacity: 0.85;
    filter: alpha(opacity=85);
}

.icon-effect:hover
{
    opacity: 1;
    -moz-opacity: 1;
    filter: alpha(opacity=100);
}


/* Final do GridView --------------- */

.tooltipInfo
{
    color: #216ead;
    font-weight: bold;
    clear: left;
    font-size: 13px;
}

.tooltipDados
{
    color: #4380b2;
    font-weight: normal;
    clear: left;
    font-size: 11px;
}



