.campo
{
    float:left;
	font-family: Arial;
	font-size: 11px;
	border: 1px solid #5b5b5b;
	line-height: 17px;
	height: 16px;
	margin-top: 5px;
	background: #f1f1f1;
	color: #000000;
	display: inherit;
	margin-right: 5px;
	padding-left: 5px;
	padding-right:5px;
}

.rotulo
{
	float: left;
	height: 17px;
	font-size: 10px;
	border: 1px solid #5b5b5b;
	background-color: #5b5b5b;
	color: #ffffff;
	padding-bottom: 2px;
	padding-left: 4px;
	width: 156px;
	line-height: 25px;
	margin-top: 5px; /*display:block;*/
	margin-right: 5px;
	font-weight: bold;
}

.rotulo2
{
	float: left;
	height: 17px;
	font-size: 10px;
	border: 1px solid #006666;
	background-color: #006666;
	color: #ffffff;
	padding-bottom: 2px;
	padding-left: 4px;
	width: 156px;
	line-height: 25px;
	margin-top: 0px; /*display:block;*/
	margin-right: 1px;
	font-weight: bold;
	margin-left: 20px;
}


.rotulo_effect
{
	float: left;
	height: 17px;
	font-size: 10px;
	border: 1px solid #19482c;
	background-color: #19482c;
	color: #ffffff;
	padding-bottom: 2px;
	padding-left: 4px;
	width: 156px;
	line-height: 25px;
	margin-top: 0px; /*display:block;*/
	margin-right: 6px;
	font-weight: bold;
	margin-left: 0px;
}

.rotulomin
{
	line-height: 25px;
	font-size: 10px;
	background-color: #5a866c;
	margin-left: 30px;
	border: 1px solid #5a866c;
	padding-right: 25px;
	padding-top: 6px;
	padding-left: 4px;
	margin-bottom: 6px;
	color: #ffffff;
	line-height: 25px;
	font-weight: bold;
	height: 17px;
}

.colunaEscondida
{
	display: none;
}


.comboBox
{
	font-family: Verdana;
	font-size: 10px;
	border: 1px solid #CCC;
	margin-bottom: 0px;
}

.label
{
	font-family: Verdana;
	font-size: 11px;
	color: Black;
	text-decoration: underline;
	font-weight: bold;
	height: 20px;
}

.labelMensagem
{
	font-family: Verdana;
	font-size: 10px;
	color: Black;
	text-align: justify;
	font-weight: bold;
	height: 20px;
}

.camporadio
{
	border: 1px solid #000000;
	background-position: #f6f6f6;
	font-family: Arial;
	font-size: 11px;
	height: 23px;
	margin-top: 0px;
	background: #f6f6f6;
	padding-left: 50px;
	margin-bottom: 0px; /*Alinhamento do texto dentro da linha*/
	vertical-align: middle;
}

.camporadio_end
{
	font-family: Arial;
	font-size: 11px;
	height: 23px;
	margin-top: 0px;
	background: #f6f6f6;
	padding-right: 40px;
	margin-bottom: 0px; /*Alinhamento do texto dentro da linha*/
	vertical-align: middle;
}

.hearderGridView
{
	background: #538065;
	font-weight: bold;
	color: #FFFFFF;
	height: 22px;
	font-size: x-small;
}

.rowGridView
{
	color: #000000;
	background: #f5f5f5;
	height: 21px;
	font-size: x-small;
}

.radio
{
	font-family: Verdana;
	font-size: 10px;
	border: 1px solid #AAA;
}

/*Classe HEADER das tabelas*/
.tab
{
	font-family: Verdana;
	font-size: x-small;
	border: 1px solid #CCC;
	height: 22px;
	background-color: #e7e7e7;
	font-weight: bold;
}

/*Classe ROW das tabelas*/
.tabrow
{
	font-family: arial;
	font-size: 8pt;
	border: 1px solid #CCC;
	height: 18px;
	text-align: center;
}

.tabrow_left
{
	font-family: arial;
	font-size: 8pt;
	border: 1px solid #CCC;
	height: 18px;
}

.form
{
	margin: 0;
	padding: 0;
}

.buttons
{
	background: #e3e3db;
	font-size: 12px;
	color: #333333;
	padding: 6px 10px;
	border-width: 2px;
	border-style: solid;
	border-color: #fff #d8d8d0 #d8d8d0 #fff;
	text-decoration: none;
	text-transform: uppercase;
	font-weight: bold;
}

div, select
{
	font-family: Arial, Verdana, Helvetica, sans-serif;
	text-decoration: none;
}

.sis_fonte_small
{
	font-size: 11px;
	font-family: Verdana, Arial, Helvetica, sans-serif;
}

/* Estilo do Accordeon*/
.accordionHeader
{
	border: 1px solid #142126;
	color: #142126;
	background-color: #eee; /*font-weight: bold;*/
	padding: 5px;
	margin-top: 5px;
	cursor: pointer;
	width: 500px;
}

.accordionHeaderSelected
{
	border: 1px solid #142126;
	color: white;
	background-color: #142126; /*font-weight: bold;*/
	padding: 5px;
	margin-top: 5px;
	cursor: pointer;
	width: 500px;
}

.accordionContent
{
	background-color: #fff;
	border: 1px solid #142126;
	border-top: none;
	padding: 5px;
	padding-top: 10px;
	width: 500px;
}
