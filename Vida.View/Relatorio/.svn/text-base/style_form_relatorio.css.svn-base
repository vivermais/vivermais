h2
{
	border-bottom: 1px dotted #cccccc;
	font-family: "Trebuchet MS" , Arial, Helvetica, sans-serif;
	margin: 0px 0px 0px 0px;
	padding: 0px 0px 0px 0px;
}

h4
{
	font-family: Arial, Helvetica, sans-serif;
	font-size: 11px;
}



.label_form
{
	float: left;
	width: 70px;
}

br
{
	clear: left;
}

legend
{
	font-family: Arial, Helvetica, sans-serif;
	font-size: 12px;
	letter-spacing: 0px;
	font-weight: bold;
	line-height: 1.3;
	color: #FFFFFF;
	padding: 7px 12px;
	padding-bottom: 10px;
	background: #303030;
	margin-top: 30px;
}


fieldset
{
	border: 1px dashed #303030;
	padding: 10px;
	margin-bottom: 20px;
}

.formulario
{
	width: 700px;
	height: auto; /*margin-left: 117px;*/
	margin-right: 0;
	padding: 10px 10px 5px 10px;
}

