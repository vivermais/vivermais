.top_menu
{
	font-family: Arial, Verdana, Sans-Serif;
	background: url( 'img/aba_menu_seguranca.png' ) no-repeat left;
	height: 45px;
	margin: 0 0px 0 0px;
	padding: 0 3px 0 55px;
	line-height: 27px;
	width: 471px;
	position: relative;
	top: -33px;
	left: 457px;
	float: left;
	z-index: 2;
}

#container_menu
{
	width: 776px;
	height: 99px;
	margin-left: -500px;
	margin-top: -300px;
	position: absolute;
	left: 56%;
	top: 131%;
	vertical-align: top;
}

.divrow_menu
{
	display: block;
	vertical-align: top;
}
.divcell_menu
{
	display: inline;
	position: relative;
	width: 172px;
	height: 39px;
	float: right;
	overflow: hidden;
	margin-bottom: 0px;
	top: 0px;
	left: 0px;
}

.divlogin
{
	display: inline;
	position: relative;
	float: left;
	width: 1001px;
	height: 143px;
	overflow: hidden;
	margin-bottom: 0px;
	top: 0px;
	left: 0px;
}
.divfoot
{
	display: inline;
	position: relative;
	width: 1001px;
	height: 29px;
	float: left;
	overflow: hidden;
	margin-bottom: 0px;
	background: #000 url( 'img/bgfoot.jpg' );
}

.logon
{
	width: 310px;
	float: left;
	font-size: 10px;
	color: #FFFFFF;
	padding: 0px 0px 0px 0px;
	position: absolute;
	top: 5px;
	left: 25px;
	margin-left: 640px;
