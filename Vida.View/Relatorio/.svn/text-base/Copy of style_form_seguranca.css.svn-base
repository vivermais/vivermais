/* Estilos do formulário - MODULO AGENDAMENTO */

body
{
	background-color: #ffffff;
}

.botoesroll
{
	float: left;
	width: auto;
	padding: 0px 20px 0 0;
	text-decoration: none;
	border: none;
}

#divbotoesposition
{
	padding: 15px 0 0 0;
}

#geral
{
	width: 950px;
	height: auto; 
	/*z-index: -1;*/
	float: inherit;
	padding-top: 50px;
	margin-left: 50px; /*background-color:#ebf0f5;*/
}

a img
{
	border: none;
	text-decoration: none;
}

#bgseguranca
{
	background-color: #ffffff;
	width: 100%;
	height: 65px;
}

.backgroundgreen
{
	background-color: #ffffff;
}


#top
{
	width: 813px;
	height: auto;
	float: inherit;
	margin-left: 50px;
	z-index: auto;
	background-color: #ffffff;
}

h2
{
	border-bottom: 1px dotted #cccccc;
	font-family: "Trebuchet MS" , Arial, Helvetica, sans-serif;
	margin: 0px 0px 0px 0px;
	padding: 0px 0px 0px 0px;
}

h4
{
	font-family: Arial, Helvetica, sans-serif;
	font-size: 9px;
}

