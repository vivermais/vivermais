/* Estilo do Accordeon*/
.accordionHeader
{
    border: 1px solid #142126;
    color: #142126;
    background-color: #eee;
    /*font-weight: bold;*/
    padding: 5px;
    margin-top: 5px;
    cursor: pointer;
    width: 500px;
    
}

.accordionHeaderSelected
{
    border: 1px solid #142126;
    color: white;
    background-color: #142126;
    /*font-weight: bold;*/
    padding: 5px;
    margin-top: 5px;
    cursor: pointer;
    width: 500px;
}

.accordionContent
{
    background-color: #fff;
    border: 1px solid #142126;
    border-top: none;
    padding: 5px;
    padding-top: 10px;
    width: 500px;
}    

