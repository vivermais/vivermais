/* Estilo do Accordeon*/
.accordionHeader
{
    border: 1px solid #c6a033;
    color: #142126;
    background-color: #c6a033;
    /*font-weight: bold;*/
    padding: 5px;
    margin-top: 5px;
    cursor: pointer;
    width: 520px;
    font-weight:bold;
    
}

.accordionHeaderSelected
{
    border: 1px solid #c6a033;
    color: white;
    background-color: #c6a033;
    /*font-weight: bold;*/
    padding: 5px;
    margin-top: 5px;
    cursor: pointer;
    width: 520px;
    font-weight:bold;
}

.accordionContent
{
    background-color: #fff;
    border: 1px solid #c6a033;
    border-top: none;
    padding: 5px;
    padding-top: 10px;
    width: 520px;}
