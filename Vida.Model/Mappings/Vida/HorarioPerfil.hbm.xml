<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ViverMais.Model">
  <class name="ViverMais.Model.HorarioPerfil" table="tb_pms_perfil_horario" >
    <id name="Codigo">
      <column name="co_horario" not-null="true"/>
      <generator class="sequence">
        <param name="sequence">ViverMais_PERFIL_HORARIO</param>
      </generator>
    </id>
    
    <many-to-one name="Perfil" class="ViverMais.Model.Perfil" column="co_perfil"></many-to-one>
    <property name="Dia" column="dia" />
    <property name="HoraInicial" column="horainicial" />
    <property name="HoraFinal" column="horafinal" />
    <property name="Bloqueado" column="bloqueado" type="YesNo"/>
  </class>
</hibernate-mapping>