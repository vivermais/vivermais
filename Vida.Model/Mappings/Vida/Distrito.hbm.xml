<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ViverMais.Model">
	<class name="ViverMais.Model.Distrito" table="tb_pms_distrito" >
		<id name="Codigo">
			<column name="co_distrito" not-null="true"/>
			<generator class="assigned" />
		</id>
		<property name="Nome" column="no_distrito"/>
    <many-to-one name="Municipio" class="ViverMais.Model.Municipio" column="co_municipio" />
	</class>
</hibernate-mapping>