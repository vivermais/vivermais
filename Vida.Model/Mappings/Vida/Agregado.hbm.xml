<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ViverMais.Model">
  <class name="ViverMais.Model.Agregado" table="tb_pms_agregado">
    <id name="Codigo">
      <column name="id_agregado" not-null="true"/>
      <generator class="assigned" />
    </id>
    <property name="Nome" column="nome"/>
    <many-to-one name="SubGrupoAgregado" class="ViverMais.Model.SubGrupoAgregado" column="id_subgrupo_agregado"/>
  </class>
</hibernate-mapping>