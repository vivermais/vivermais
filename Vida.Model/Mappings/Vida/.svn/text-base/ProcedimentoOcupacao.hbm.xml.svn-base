<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Vida.Model">
  <class name="Vida.Model.ProcedimentoOcupacao" table="tb_pms_procedimento_ocupacao">
    <composite-id>
      <key-many-to-one name="Procedimento" class="Vida.Model.Procedimento" column="co_procedimento" lazy="false"/>
      <key-many-to-one name="CBO" class="Vida.Model.CBO" column="co_ocupacao" lazy="false"/>
