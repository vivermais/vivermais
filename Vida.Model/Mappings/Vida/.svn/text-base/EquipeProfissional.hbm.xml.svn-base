    <many-to-one name="EstabelecimentoSaude" class="Vida.Model.EstabelecimentoSaude" column="unidade_id" />
    <many-to-one name="CBO" class="Vida.Model.CBO" column="cod_cbo" />
    <property name="VinculoSUS" column="tp_sus_nao_sus" />
    <property name="IdentificacaoVinculo" column="ind_vinc" />
    <property name="EquipeMinima" column="fl_equipeminima" />
