<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Vida.Model">
	<class name="Vida.Model.Operacao" table="tb_pms_operacao" >
		<id name="Codigo">
			<column name="co_operacao" not-null="true"/>
			<generator class="assigned" />
		</id>
		<property name="Nome" column="no_operacao"/>
    <many-to-one name="Modulo" class="Vida.Model.Modulo" column="co_sistema" ></many-to-one>
	</class>
</hibernate-mapping>