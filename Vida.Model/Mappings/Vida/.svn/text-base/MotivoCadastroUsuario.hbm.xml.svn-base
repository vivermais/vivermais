<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Vida.Model">
	<class name="Vida.Model.MotivoCadastroPaciente" table="rl_ms_usuario_motivo">
		<composite-id >
			<key-many-to-one name="Paciente" class="Vida.Model.Paciente" column="co_usuario"/>
			<key-many-to-one name="Motivo" class="Vida.Model.MotivoCadastro" column="co_motivo"/>
			<key-property name="Cnes" column="co_cnes"/>
		</composite-id>
    <property name="CodigoDocumentoReferencia" column="CO_DOC_REF"/>
    <property name="CnsMae" column="CO_NUMERO_CARTAO_MAE"/>
    <property name="NumeroDocumento" column="NU_DOCUMENTO"/>
    <property name="DataOperacao" column="DT_OPERACAO"/>
    <property name="Controle" column="ST_CONTROLE"/>
    <property name="Excluido" column="ST_EXCLUIDO"/>
    <property name="NumeroVersao" column="NU_VERSAO"/>
  </class>
</hibernate-mapping>