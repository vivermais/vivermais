<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ViverMais.Model">
	<class name="ViverMais.Model.EnderecoUsuario" table="rl_ms_usuario_endereco">
		<composite-id>
			<key-many-to-one name="Endereco" class="ViverMais.Model.Endereco" column="co_endereco"/>
			<!--<key-many-to-one name="Paciente" class="ViverMais.Model.Paciente" column="co_usuario"/>-->
			<key-property name="CodigoPaciente" column="co_usuario"/>
			<key-many-to-one name="TipoEndereco" class="ViverMais.Model.TipoEndereco" column="co_tipo_endereco"/>
    </composite-id>
    <property name="Controle" column="ST_CONTROLE"/>
    <property name="Excluido" column="ST_EXCLUIDO"/>
    <property name="Operacao" column="DT_OPERACAO"/>
    <property name="Vinculo" column="ST_VINCULO"/>
	</class>
</hibernate-mapping>