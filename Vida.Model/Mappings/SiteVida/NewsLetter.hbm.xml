<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ViverMais.Model">

  <class name="ViverMais.Model.NewsLetter" table="site_usuarioemail">
    <id name="Codigo" >
      <column name="CO_USUARIO" not-null="true"/>
      <generator class="sequence">
        <param name="sequence">SITE_USUARIOEMAIL_SEQUENCE</param>
      </generator>
    </id>
    <!--<property name="Codigo" column="co_usuario" />-->
    <property name="Nome"   column="nome" />
    <property name="Email"  column="email" />
    <property name="DataCriacao"  column="datacriacao" />

  </class>
</hibernate-mapping>