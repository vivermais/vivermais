<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Vida.Model">
  <class name="Vida.Model.PrescricaoProcedimentoNaoFaturavel" table="URG_PRESCRICAO_PROCEDIMENTONF">
    <!--<composite-id unsaved-value="none">
      <key-many-to-one name="Prescricao" class="Vida.Model.Prescricao" column="CO_PRESCRICAO" lazy="false"/>
      <key-many-to-one name="Procedimento" class="Vida.Model.ProcedimentoNaoFaturavel"  column="CO_PROCEDIMENTONAOFATURAVEL" lazy="false"/>
    </composite-id>-->
    <id name="Codigo">
      <column name="codigo" not-null="true"/>
      <generator class="sequence">
        <param name="sequence">urg_prescricao_naofat_sq</param>
      </generator>
    </id>
    <many-to-one name="Prescricao" class="Vida.Model.Prescricao" column="CO_PRESCRICAO" />
    <many-to-one name="Procedimento" class="Vida.Model.ProcedimentoNaoFaturavel"  column="CO_PROCEDIMENTONAOFATURAVEL" />
    <property name="Intervalo" column="intervalo" />
    <property name="Suspenso" column="suspenso" type="YesNo"/>
    <property name="CodigoProfissional" column="CO_PROFISSIONAL" />
    <property name="CBOProfissional" column="cbo_profissional" />
    <property name="Data" column="data" />
    <property name="AplicacaoUnica" column="aplicacaounica" type="YesNo"/>
  </class>
