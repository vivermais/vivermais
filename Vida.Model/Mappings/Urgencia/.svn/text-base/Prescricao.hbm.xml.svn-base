<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Vida.Model">
  <class name="Vida.Model.Prescricao" table="URG_PRESCRICAO">
    <id name="Codigo">
      <column name="CO_PRESCRICAO" not-null="true"/>
      <generator class="sequence">
        <param name="sequence">URG_PRESCRICAO_SEQUENCE</param>
      </generator>
    </id>
    <property name="Data" column="data_criacao" />
    <property name="DataVigencia" column="data_vigencia" />
	  <many-to-one name="Prontuario" class="Vida.Model.Prontuario" column="CO_PRONTUARIO" />
    <!--<many-to-one name="EvolucaoMedica" class="Vida.Model.EvolucaoMedica" column="id_evolucaomedica" />-->
    <!--<many-to-one name="EvolucaoEnfermagem" class="Vida.Model.EvolucaoEnfermagem" column="id_evolucaoenfermagem" />-->
    <property name="Profissional" column="CO_PROFISSIONAL" />
    <property name="CBOProfissional" column="cbo_profissional" />
    <property name="Status" column="status" />
    <property name="UltimaDataValida" column="ultimadatavalida" />
  </class>
</hibernate-mapping>