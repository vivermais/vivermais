<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ViverMais.Model">
  <class name="ViverMais.Model.PacienteUrgence" table="URG_PACIENTE">
    <id name="Codigo">
      <column name="CO_PACIENTE" not-null="true"/>
      <generator class="sequence">
        <param name="sequence">URG_PACIENTE_SEQUENCE</param>
      </generator>
    </id>
    
    <property name="CodigoViverMais" column="CO_PACIENTEViverMais" />
    <property name="Nome" column="nome" />
    <property name="Descricao" column="descricao" type="StringClob" />
    <property name="Sexo" column="sexo" />
    <property name="Simplificado" column="simplificado" type="YesNo" />
    
  </class>
</hibernate-mapping>