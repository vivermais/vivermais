<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ViverMais.Model">
  <class name="ViverMais.Model.EvolucaoEnfermagem" table="URG_EVOLUCAOENFERMAGEM">
    <id name="Codigo">
      <column name="CO_EVOLUCAO" not-null="true"/>
      <generator class="sequence">
        <param name="sequence">URG_EVENFERMAGEM_SEQUENCE</param>
      </generator>
    </id>
    <many-to-one name="Prontuario" class="ViverMais.Model.Prontuario" column="CO_PRONTUARIO" lazy="false"/>
    <property name="CodigoProfissional" column="CO_PROFISSIONAL" />
    <property name="CBOProfissional" column="cbo_profissional" />
    
    <property name="Observacao" column="observacao" type="StringClob"/>
	  <property name="Data" column="data" />
  </class>
</hibernate-mapping>