<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ViverMais.Model">
  <class name="ViverMais.Model.LogVacina" table="vcn_logeventos">
    <id name="Codigo">
      <column name="id" not-null="true"/>
      <generator class="sequence">
        <param name="sequence">VCN_LOGEVENTOS_SEQUENCE</param>
      </generator>
    </id>
    <!--<id name="Codigo">
      <column name="id" not-null="true"/>
      <generator class="assigned" />
    </id>-->
    <property name="CodigoUsuario" column="co_usuario" />
    <property name="CodigoEvento" column="co_evento" />
    <property name="Valor" column="valor" />
    <property name="Data" column="data" />
  </class>
</hibernate-mapping>
