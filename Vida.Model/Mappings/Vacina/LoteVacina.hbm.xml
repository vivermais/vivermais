<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ViverMais.Model">
  <class name="ViverMais.Model.LoteVacina" table="VCN_LOTE">
    <!--<id name="Codigo">
      <column name="CO_LOTE" not-null="true"/>
      <generator class="assigned" />
    </id>-->
    <id name="Codigo">
      <column name="CO_LOTE" not-null="true"/>
      <generator class="sequence">
        <param name="sequence">VCN_LOTE_SEQUENCE</param>
      </generator>
    </id>
    <many-to-one name="ItemVacina" class="ViverMais.Model.ItemVacina" column="CO_ITEM" />
    <property name="Identificacao" column="IDENTIFICACAO" />
    <property name="DataValidade" column="DATAVALIDADE" />
  </class>
</hibernate-mapping>
