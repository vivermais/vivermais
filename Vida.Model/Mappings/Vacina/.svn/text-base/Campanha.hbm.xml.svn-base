<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Vida.Model">
  <class name="Vida.Model.Campanha" table="VCN_Campanha">
    <id name="Codigo">
      <column name="co_campanha" not-null="true"/>
      <generator class="assigned" />
    </id>
    <property name="Nome" column="nome" />
    <!--<property name="Ano" column="ano" />
    <property name="Mes" column="mes" />-->
    <property name="FaixaEtariaInicial" column="faixaEtariaInicial" />
    <property name="FaixaEtariaFinal" column="faixaEtariaFinal" />
    <property name="DataInicio" column="DATAINICIO" />
    <property name="DataFim" column="DATAFIM" />
    <property name="UnidadeFaixaInicial" column="UNIDADEFAIXAINICIAL" />
    <property name="UnidadeFaixaFinal" column="UNIDADEFAIXAFINAL" />
    <property name="Sexo" column="sexo" />
    <property name="Meta" column="META"/>
    <property name="Status" column="STATUS" />
  </class>
</hibernate-mapping>