<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Vida.Model">
  <class name="Vida.Model.MotivoMovimentoVacina" table="vcn_motivomovimento">
    <id name="Codigo">
      <column name="co_motivo" not-null="true"/>
      <generator class="assigned" />
    </id>
    <property name="Nome" column="nome" />
    <many-to-one name="TipoMovimento" class="Vida.Model.TipoMovimentoVacina" column="co_tipomovimento" />
  </class>
</hibernate-mapping>