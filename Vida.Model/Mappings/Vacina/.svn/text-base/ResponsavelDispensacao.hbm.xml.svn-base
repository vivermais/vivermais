<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Vida.Model">
  <class name="Vida.Model.ResponsavelDispensacao" table="VCN_Responsavel_Dispensacao">
    <id name="Codigo">
      <column name="co_responsavel" not-null="true"/>
      <generator class="assigned" />
    </id>
    <many-to-one name="Dispensacao" class="Vida.Model.DispensacaoVacina" column="CO_DISPENSACAO" />
    <property name="Telefone" column="telefone" />
    <property name="Email" column="email" />
  </class>
</hibernate-mapping>