<?xml version="1.0" encoding="utf-8" ?> 
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ViverMais.Model">
  <class name="ViverMais.Model.UsuarioVacina" table="vcn_usuario">
    <id name="Codigo">
      <column name="codigo" not-null="true"/>
      <generator class="sequence">
        <param name="sequence">vcn_usuario_sala</param>
      </generator>
    </id>
    
    <many-to-one name="Sala" class="ViverMais.Model.SalaVacina" column="co_sala"></many-to-one>
    <many-to-one name="Usuario" class="ViverMais.Model.Usuario" column="co_usuario"></many-to-one>
    <property name="Responsavel" column="responsavel" type="YesNo" />
    
  </class>
</hibernate-mapping>