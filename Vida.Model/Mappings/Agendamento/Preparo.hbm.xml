<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ViverMais.Model">
  <class name="ViverMais.Model.Preparo" table="AGD_PREPARO">
    <id name="Codigo">
       <column name="CO_PREPARO" not-null="true"/>
       <generator class="sequence">
           <param name="sequence">AGD_PREPARO_SEQUENCE</param>
       </generator>
    </id>
    <property name="Descricao" column="DESCRICAO" />    
  </class>
</hibernate-mapping>