<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ViverMais.Model">
  <class name="ViverMais.Model.FaixaProcedimento" table="AGD_FAIXA_PROCEDIMENTO">
    <composite-id>
      <key-many-to-one name="Faixa" class="ViverMais.Model.Faixa" column="co_faixa" />
      <key-property name="Id_Procedimento" column="co_procedimento" />
      <key-property name="Id_Unidade" column="cnes" />
    </composite-id>
    <property name="ValidadeInicial" column="validade_inicial" />
    <property name="ValidadeFinal" column="validade_final" />
  </class>
</hibernate-mapping>