<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Vida.Model">
  <class name="Vida.Model.ParametroCotas" table="ParametroCotas">
    <id name="Codigo">
      <column name="id_parametrocotas" not-null="true"/>
      <generator class="identity" />
    </id>
	<property name="TipoAgenda" column="id_tipoagenda" />
	<property name="ID_Unidade" column="id_unidade" />
    <property name="Id_Procedimento" column="id_procedimento" />
    <property name="Id_Grupo" column="id_grupo" />
    <property name="QuantidadeTotal" column="qtd_total" />
    <property name="QuantidadeAgendada" column="qtd_agendada" />
    <property name="Competencia" column="competencia" />
  </class>
</hibernate-mapping>