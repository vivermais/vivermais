<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Vida.Model">
  <class name="Vida.Model.TipoProcedimento" table="AGD_TIPOPROCEDIMENTO">
    <id name="Procedimento">
       <column name="CO_PROCEDIMENTO" not-null="true"/>
       <generator class="assigned" />
    </id>    
    <property name="Tipo" column="TIPO" />
    <bag name="Preparos" table="AGD_PROCEDIMENTO_PREPARO">
      <key column="CO_PROCEDIMENTO"/>
      <many-to-many column="CO_PREPARO" class="Vida.Model.Preparo" />
    </bag>
  </class>
</hibernate-mapping>