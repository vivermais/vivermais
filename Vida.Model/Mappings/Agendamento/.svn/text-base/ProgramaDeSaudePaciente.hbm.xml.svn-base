  <class name="Vida.Model.ProgramaDeSaudePaciente" table="AGD_PROGRAMA_PACIENTE">
    <composite-id>
      <key-many-to-one name="ProgramaDeSaude" class="Vida.Model.ProgramaDeSaude" column="co_programa"/>
      <key-many-to-one name="Paciente" class="Vida.Model.Paciente" column="co_usuario"/>
      <!--<key-property name="CodigoPaciente" column="co_usuario"/>-->
      <!--<key-many-to-one name="TipoEndereco" class="Vida.Model.TipoEndereco" column="co_tipo_endereco"/>-->
    </composite-id>
    <property name="Ativo" column="ativo"/>
  </class>
</hibernate-mapping>