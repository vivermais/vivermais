<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Vida.Model">
  <class name="Vida.Model.PacienteLaboratorio" table="tb_paciente">
    <id name="Codigo">
      <column name="cd_paciente_pk" not-null="true"/>
      <generator class="assigned" />
    </id>
    <property name="Nome" column="nm_paciente" />
    <property name="Rg" column="nr_rg" />
    <property name="CartaoSus" column="nr_cartao_sus" />
    <property name="DataNascimento" column="dt_nascimento" />
  </class>
</hibernate-mapping>