<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ViverMais.Model">
  <class name="ViverMais.Model.EventoFarmacia" table="far_eventos">
    <id name="Codigo">
      <column name="id" not-null="true"/>
      <generator class="sequence" >
        <param name="sequence">FAR_EVENTOS_SEQUENCE</param>
      </generator>
    </id>
    <property name="Nome" column="evento" />
  </class>
</hibernate-mapping>
