<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ViverMais.Model">
  <class name="ViverMais.Model.UnidadeDistritoRM" table="far_unidadedistrito">
    <id name="Codigo">
      <column name="id_unidadedistrito" not-null="true"/>
      <generator class="sequence">
        <param name="sequence">FAR_UNIDADEDISTRITO_SEQUENCE</param>
      </generator>
    </id>
    <many-to-one name="Unidade" class="ViverMais.Model.EstabelecimentoSaude" column="id_unidade" />
    <many-to-one name="Distrito" class="ViverMais.Model.Distrito" column="id_distrito" />
  </class>
</hibernate-mapping>