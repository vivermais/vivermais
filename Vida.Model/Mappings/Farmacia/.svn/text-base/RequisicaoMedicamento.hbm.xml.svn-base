<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Vida.Model">
  <class name="Vida.Model.RequisicaoMedicamento" table="far_requisicao">
    <id name="Codigo">
      <column name="id_requisicao" not-null="true"/>
      <generator class="sequence" >
        <param name="sequence">FAR_REQUISICAO_SEQUENCE</param>
      </generator>
    </id>
	  <many-to-one name="Farmacia" class="Vida.Model.Farmacia" column="id_farmacia" />
	  <property name="DataCriacao" column="datacriacao" />
    <property name="DataEnvio" column="dataenvio" />
	  <property name="DataAtendida" column="dataatendida" />
	  <property name="Data_Status" column="data_status" />
	  <property name="Data_Autorizacao" column="data_autorizacao" />
	  <property name="Data_Fornecimento" column="data_fornecimento" />
	  <property name="Obs" column="obs" />
	  <property name="Cod_Status" column="cod_status" />
  </class>
</hibernate-mapping>