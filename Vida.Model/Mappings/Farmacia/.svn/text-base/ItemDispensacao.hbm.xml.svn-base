<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Vida.Model">
	<class name="Vida.Model.ItemDispensacao" table="far_itemdispensacao">
    <id name="Codigo">
      <column name="id_item_dispensacao" not-null="true"/>
      <generator class="sequence" >
        <param name="sequence">FAR_ITEMDISPENSACAO_SEQUENCE</param>
      </generator>
    </id>    
    <many-to-one name="Dispensacao" class="Vida.Model.Dispensacao" column="id_dispensacao"/>			
		<many-to-one name="LoteMedicamento" class="Vida.Model.LoteMedicamento" column="id_lotemedicamento"/>
<!--<property name="DataAtendimento" column="dataatendimento" />		-->
		<property name="QtdDispensada" column="qtddispensada" />
		<property name="QtdDias" column="qtddias" />
    <property name="Observacao" column="observacao" />
	</class>
</hibernate-mapping>
