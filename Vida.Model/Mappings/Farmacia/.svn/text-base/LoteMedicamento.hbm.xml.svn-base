<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Vida.Model">
	<class name="Vida.Model.LoteMedicamento" table="far_lotemedicamento">
		<id name="Codigo">
			<column name="id_lotemedicamento" not-null="true"/>
      <generator class="sequence" >
         <param name="sequence">FAR_LOTEMEDICAMENTO_SEQUENCE</param>
      </generator>
		</id>
		<property name="Lote" column="lote" />
		<property name="Validade" column="validade" />
		<many-to-one name="Medicamento" class="Vida.Model.Medicamento" column="id_medicamento" />
		<many-to-one name="Fabricante" class="Vida.Model.FabricanteMedicamento" column="id_fabricante" />
	</class>
</hibernate-mapping>
