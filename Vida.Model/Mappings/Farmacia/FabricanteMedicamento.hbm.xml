<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ViverMais.Model">
  <class name="ViverMais.Model.FabricanteMedicamento" table="far_fabricante">
    <id name="Codigo">
      <column name="id_fabricante" not-null="true"/>
      <generator class="sequence" >
        <param name="sequence">FAR_FABRICANTE_SEQUENCE</param>
      </generator>
    </id>
    <property name="Nome" column="fabricante" />
  </class>
</hibernate-mapping>