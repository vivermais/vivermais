<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Vida.Model">
  <class name="Vida.Model.SituacaoMovimento" table="far_situacao">
    <id name="Codigo">
      <column name="id_situacao" not-null="true"/>
      <generator class="sequence" >
        <param name="sequence">FAR_SITUACAO_SEQUENCE</param>
      </generator>
    </id>
    <property name="Nome" column="situacao" />
  </class>
</hibernate-mapping>