using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace Vida.Model
{
    [Serializable]
    public class Operacao
    {
        int codigo;

        public virtual int Codigo
        {
            get { return codigo; }
            set { codigo = value; }
        }

        string nome;

        public virtual string Nome
        {
            get { return nome; }
            set { nome = value; }
        }

        Modulo modulo;

        public virtual Modulo Modulo
        {
            get { return modulo; }
            set { modulo = value; }
        }

        public Operacao()
        {

        }
    }
}
